<?xml version="1.0" encoding="utf-8"?>
<Defs>
  <!-- Civil -->
  <ThingDef ParentName="AncientHatMakeableBase">
    <defName>AR_Headscarf</defName>
    <label>egyptian headscarf</label>
    <description>A rectangular piece of cloth worn around the head of common people from the Nile. Perfect for keeping people cool in the hot desert heat.</description>
    <graphicData>
      <texPath>Items/Apparel/HeadWrapOnMap</texPath>
      <graphicClass>Graphic_Single</graphicClass>
    </graphicData>
    <costStuffCount>25</costStuffCount>
    <stuffCategories>
      <li>Fabric</li>
      <li>Leathery</li>
    </stuffCategories>
    <statBases>
      <WorkToMake>850</WorkToMake>
      <Mass>0.1</Mass>
      <StuffEffectMultiplierArmor>0.1</StuffEffectMultiplierArmor>
      <StuffEffectMultiplierInsulation_Heat>1</StuffEffectMultiplierInsulation_Heat>
      <EquipDelay>0.8</EquipDelay>
    </statBases>
    <apparel>
      <bodyPartGroups>
        <li>UpperHead</li>
      </bodyPartGroups>
      <wornGraphicPath>Items/Apparel/Hats/HeadWrap</wornGraphicPath>
      <layers>
        <li>Overhead</li>
      </layers>
      <tags>
        <li>EgyptianBasic</li>
        <!-- Mod Support -->
        <li>BeetWear</li>
        <li>JeubWear</li>
        <li>ST_GnomeClothing</li>
        <li>islam_head</li>
        <li>Muslim_apparel</li>
      </tags>
      <defaultOutfitTags>
        <li>Worker</li>
        <li>Slave</li>
      </defaultOutfitTags>
      <canBeGeneratedToSatisfyWarmth Inherit="False">false</canBeGeneratedToSatisfyWarmth>
      <countsAsClothingForNudity>false</countsAsClothingForNudity>
    </apparel>
    <recipeMaker>
      <researchPrerequisite>AR_Project_Oriental_Culture</researchPrerequisite>
      <recipeUsers>
        <li>CraftingSpot</li>
        <!-- The other production centers should be inherited by the Base -->
      </recipeUsers>
    </recipeMaker>
    <colorGenerator Class="ColorGenerator_Options">
      <options>
        <li>
          <weight>20</weight>
          <only>(1,1,1,1)</only>
        </li>
        <li>
          <weight>5</weight>
          <only>(0.7,0.63,0.2,1)</only>
        </li>
      </options>
    </colorGenerator>
  </ThingDef>
  <ThingDef ParentName="AncientHatMakeableBase">
    <defName>AR_SkullCap</defName>
    <label>skull helmet</label>
    <description>A cap for the head. Initially used only by men, thousands of years have caused it to be seen as unisex for most primitive societies. Countless tribes across the galaxy employ it for basic heat protection.</description>
    <graphicData>
      <texPath>Items/Apparel/CapOnMap</texPath>
      <graphicClass>Graphic_Single</graphicClass>
    </graphicData>
    <costStuffCount>15</costStuffCount>
    <stuffCategories>
      <li>Fabric</li>
      <li>Leathery</li>
    </stuffCategories>
    <statBases>
      <WorkToMake>1000</WorkToMake>
      <Mass>0.01</Mass>
      <StuffEffectMultiplierInsulation_Heat>1</StuffEffectMultiplierInsulation_Heat>
      <EquipDelay>0.1</EquipDelay>
      <StuffEffectMultiplierArmor>0.4</StuffEffectMultiplierArmor>
    </statBases>
    <apparel>
      <bodyPartGroups>
        <li>UpperHead</li>
      </bodyPartGroups>
      <wornGraphicPath>Items/Apparel/Hats/Cap</wornGraphicPath>
      <layers>
        <li>Overhead</li>
      </layers>
      <tags>
        <li>Neolithic</li>
        <li>Tribal</li>
        <li>EgyptianBasic</li>
        <li>EgyptianLight</li>
        <li>OrientalLight</li>
      </tags>
      <!-- <gender>Male</gender> -->
      <defaultOutfitTags>
        <li>Worker</li>
        <li>Soldier</li>
        <li>Slave</li>
      </defaultOutfitTags>
      <canBeGeneratedToSatisfyWarmth Inherit="False">false</canBeGeneratedToSatisfyWarmth>
      <countsAsClothingForNudity>false</countsAsClothingForNudity>
    </apparel>
    <recipeMaker>
      <researchPrerequisite>AR_Project_Oriental_Culture</researchPrerequisite>
      <recipeUsers>
        <li>CraftingSpot</li>
        <!-- Should inherit the other production centers from the Base -->
      </recipeUsers>
    </recipeMaker>
    <colorGenerator Class="ColorGenerator_StandardApparel" />
  </ThingDef>
